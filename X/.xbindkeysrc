# "exec" is used for potentially long-running commands

"speaker-toggle"
    XF86AudioMute

"yubi toggle"
    XF86Tools

"volume-down"
    XF86AudioLowerVolume

"volume-up"
   XF86AudioRaiseVolume

"mic-toggle"
   XF86AudioMicMute

"pavucontrol"
   Shift+Mod4 + o

"xbacklight -inc 5"
   XF86MonBrightnessUp

"xbacklight -dec 5"
   XF86MonBrightnessDown

"wlan-toggle"
   XF86WLAN

"reenable-monitor"
   XF86Display

"exec firefox"
    Mod4 + f

"exec alacritty-window"
    Mod4 + Return

# Backup if alacritty/urxvtd crashes
"exec urxvt -e bash"
    Shift+Mod4 + Return

"dmenu_run"
    Mod4 + v

"exec paste-buffer"
    Mod4 + p

"xset s activate"
    Control+Alt + l

"xset s activate"
    Print
