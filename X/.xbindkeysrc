# "exec" is used for potentially long-running commands

"speaker-toggle"
    XF86AudioMute

"yubi toggle"
    XF86Tools

"amixer sset Master 3%-"
    XF86AudioLowerVolume

"amixer sset Master 3%+"
   XF86AudioRaiseVolume

"mic-toggle"
   XF86AudioMicMute

"switch-pulse-output"
   Shift+Mod4 + o

"xbacklight -inc 5"
   XF86MonBrightnessUp

"xbacklight -dec 5"
   XF86MonBrightnessDown

"wlan-toggle"
   XF86WLAN

"/usr/bin/sudo /usr/local/bin/suspend"
    Control+Mod4 + s

"exec firefox"
    Mod4 + f

"exec urxvtc"
    Mod4 + Return

# Backup if urxvtd crashes
"exec urxvt -e bash"
    Shift+Mod4 + Return

"dmenu_run"
    Mod4 + v

"exec paste-buffer"
    Mod4 + p

"xset s activate"
    Control+Alt + l

"xset s activate"
    Print
