# "exec" is used for potentially long-running commands

# ctrl+alt+shift+left
"pamixer --toggle-mute"
    m:0xd + c:113

# ctrl+alt+shift+down
"pamixer --decrease 3"
    m:0xd + c:116

# ctrl+alt+shift+up
"pamixer --increase 3"
    m:0xd + c:111

# alt+shift+o
"switch-pulse-output"
    m:0x9 + c:32

# alt+shift+s
"sudo pm-suspend --auto-quirks"
    m:0x9 + c:39

"exec qutebrowser"
    Mod4 + q

"exec firefox"
    Mod4 + f

"exec urxvtc"
    Mod4 + Return

# Backup if urxvtd crashes
"exec urxvt -e bash"
    Shift+Mod4 + Return

# alt+v
"dmenu_run"
    Mod4 + v

# alt+p
"exec paste-buffer"
    Mod4 + p

# ctrl+alt+l
"exec i3lock -c 000000"
    Control+Alt + l

# ctrl+alt+printscreen
"exec i3lock -c 000000"
    Print
