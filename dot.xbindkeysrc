"rhythmbox-client --previous"
    XF86AudioPrev

"rhythmbox-client --pause"
    XF86AudioStop

"rhythmbox-client --play-pause"
    XF86AudioPlay

"rhythmbox-client --next"
    XF86AudioNext

"amixer set Master toggle"
    XF86AudioMute

"amixer set Master 3%-"
    XF86AudioLowerVolume

"amixer set Master 3%+"
    XF86AudioRaiseVolume
