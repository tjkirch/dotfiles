"mpc --no-status prev"
    XF86AudioPrev

"mpc --no-status pause"
    XF86AudioStop

"mpc --no-status toggle"
    XF86AudioPlay

"mpc --no-status next"
    XF86AudioNext

"amixer set Master toggle"
    XF86AudioMute

"amixer set Master 3%-"
    XF86AudioLowerVolume

"amixer set Master 3%+"
    XF86AudioRaiseVolume

"urxvtc"
    Alt + t

"firefox"
    Alt + f

"gvim"
    Alt + v
