"ncmpcpp toggle"
    XF86AudioPlay

"ncmpcpp stop"
    XF86AudioStop

"ncmpcpp prev"
    XF86AudioPrev

"ncmpcpp next"
    XF86AudioNext

"amixer set Master toggle"
    XF86AudioMute

"amixer set Master 3%-"
    XF86AudioLowerVolume

"amixer set Master 3%+"
    XF86AudioRaiseVolume

"sudo pm-suspend --auto-quirks"
    m:0x0 + c:194

"firefox"
    m:0x8 + c:41

"chromium"
    m:0x8 + c:54

"urxvt"
    m:0x8 + c:36

"dmenu_run"
    m:0x8 + c:55
