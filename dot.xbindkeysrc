"ncmpcpp toggle"
    XF86AudioPlay

"ncmpcpp stop"
    XF86AudioStop

"ncmpcpp prev"
    XF86AudioPrev

"ncmpcpp next"
    XF86AudioNext

"pamixer set Master toggle"
    XF86AudioMute

"pamixer --decrease 3"
    XF86AudioLowerVolume

"pamixer --increase 3"
    XF86AudioRaiseVolume

"switch-pulse-output"
    XF86Tools

"firefox"
    m:0x8 + c:41

"chromium"
    m:0x8 + c:54

"urxvt"
    m:0x8 + c:36

"dmenu_run"
    m:0x8 + c:55

"sudo /usr/bin/systemctl suspend"
    XF86Eject
